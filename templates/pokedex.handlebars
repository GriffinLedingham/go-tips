<!DOCTYPE html>
<html>
  <head>
    <link rel="stylesheet" type="text/css" href="styles/styles.css">
    <link href='https://fonts.googleapis.com/css?family=Lato' rel='stylesheet' type='text/css'>
    <meta charset="utf-8">
    <meta property="og:description" content="Find Pokemon GO evolution, stats and moves info on PoGoDex! Gen 4 is now live, find out more!" />
    <meta property="og:title" content="PoGoDex | Pokemon GO Evolution and Stat Info" />
    <meta name="Description" CONTENT="Find Pokemon GO evolution, stats, and moves info on PoGoDex! Gen 4 is now live, find out more!.">
    <title>PoGoDex | Pokemon GO Stats and Information</title>
  </head>
  <body>
    {{{header}}}
    {{{left_nav}}}
    <div id="content_wrapper">
      <div id="contents" class="pk-list">
        <div id="input_wrapper"><input id="search" class="search" placeholder="Search" /></div>
        <ul class="list">
          {{#pokemon}}
          <a class="pokedex-item" href="/pokemon/{{name}}">
            <div class="pk-glow"></div>
            <div>
              <img class="pk-sprite" src="/images/pokemon_icons/pokemon_icon_{{id_pad}}_{{form}}.png">
            </div>
            <span class="pk-name">{{name_clean}}</span>
            <span class="pk-type">{{type}}</span>
          </a>
          {{/pokemon}}
        </ul>
      </div>
      {{{footer}}}
    </div>
  </body>
  <script src="/scripts/list.js"></script>
  <script>
    var options = {
      valueNames: [ 'pk-name', 'pk-type' ]
    };

    var pokeList = new List('content_wrapper', options);
    window.onload = function() {
      document.getElementById("search").focus();
    };
    (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
    (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
    m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
    })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

    ga('create', 'UA-80936410-1', 'auto');
    ga('send', 'pageview');

  </script>
</html>